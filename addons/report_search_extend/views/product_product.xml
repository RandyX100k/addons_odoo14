<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="product_product_tree_view_view_inherit_rq" model="ir.ui.view">
        <field name="name">product.product.view.tree.inherit.rq</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_product_tree_view" />
        <field name="arch" type="xml">

            <xpath expr="//tree//field[@name='nro_motor']" position="attributes">
                <attribute name="optional">hidden</attribute>
            </xpath>

            <xpath expr="//tree//field[@name='nro_chasis']" position="attributes">
                <attribute name="optional">hidden</attribute>
            </xpath>

            <xpath expr="//tree//field[@name='modelo']" position="attributes">
                <attribute name="optional">hidden</attribute>
            </xpath>

            <xpath expr="//tree//field[@name='colores']" position="attributes">
                <attribute name="optional">hidden</attribute>
            </xpath>

            <xpath expr="//tree//field[@name='nro_dua']" position="attributes">
                <attribute name="optional">hidden</attribute>
            </xpath>

            <xpath expr="//tree//field[@name='qty_available']" position="attributes">
                <attribute name="optional">hidden</attribute>
            </xpath>

        </field>
    </record>

</odoo>